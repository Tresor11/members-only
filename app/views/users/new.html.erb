<%=provide(:title , "sign up")%>

<%= form_for(@user) do |f| %>
<%= f.label :name %>
<%= f.text_field :name %>

<%= f.label :email %>
<%= f.email_field :email %>

<%= f.label :password %>
<%= f.password_field :password %>

<%= f.label :password_confirmation, "confirmation" %>
<%= f.password_field :password_confirmation %>

<%= f.submit "create acount"%>
<% end %>
